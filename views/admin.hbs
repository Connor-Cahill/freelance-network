<h1>Admin Dashboard</h1>


<h2>Posts: </h2>
<div class="admin-post-container">
    {{#each posts}}
        <h3>{{this.title}}</h3>
        <p>{{this.content}}</p>
        <form action="/admin/posts/{{this._id}}?_method=DELETE" method="post">
            <button type="submit">Delete</button>
        </form>
        <hr>
    {{/each}}
</div>
